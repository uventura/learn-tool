<%-include('../partials/header.ejs')%>
<%-include('../partials/navbar.ejs')%>

<div class="container" style="margin-top:1.5rem">
  <h1>Groups Founded: <i class="fa-solid fa-user-group"></i></h1>
  <div class="list-group">
    <%groups.forEach(group=>{%>
      <%
        space = encodeURIComponent(' ')
        encode_title = encodeURIComponent(group.title)
          .replaceAll(space, '-')
      %>
      <a href="/group/<%=encode_title%>" class="list-group-item list-group-item-action"><%=group.title%></a>
    <%})%>
  </div>
</div>

<%-include('../partials/footer.ejs')%>
